FROM ubuntu
RUN apt update -y
RUN apt install maven -y
RUN mkdir raham
RUN touch raham/abc.txt
COPY index.html /tmp
COPY apache-maven-3.6.3-bin.tar.gz /tmp
ADD https://dlcdn.apache.org/maven/maven-3/3.6.3/binaries/apache-maven-3.6.3-bin.tar.gz /opt

